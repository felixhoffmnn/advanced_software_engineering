    "def keyword_search(keyword, max_results=3):\n",
    "    keyword_request = http_request(\n",
    "        f\"https://gnews.io/api/v4/search?q={keyword}&token={API_key}&lang=de&country=de&max={max_results}\"\n",
    "    )\n",
    "    return keyword_request"
